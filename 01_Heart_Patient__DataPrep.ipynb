      "source": [
        "There should only be 3 different values - NO, YES, and UNKNOWN"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "UzMrK13YgxhD"
      },
      "source": [
        "# clean up 'Patient_Smoker' values\n",
        "def smoker(r):\n",
        "  if (r == \"NO\") or (r == \"NO \"):\n",
        "    return 'NO'\n",
        "  elif (r == \"YES\") or (r == \"YES \") or (r == \"YESS\") or (r == \"YESS \"):\n",
        "    return 'YES'\n",
        "  else:\n",
        "    return 'UNKNOWN'"
      ],
      "execution_count": 16,
      "outputs": []
    },
